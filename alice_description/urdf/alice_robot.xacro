<?xml version="1.0"?>

<robot name="alice_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="pi" value="3.141592654" />
   <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find alice_description)/urdf/soccer_gazebo.gazebo" />
  <xacro:include filename="$(find alice_description)/urdf/materials.xacro" />

  <!-- Used for fixing robot -->
<!--
  <link name="world">
  </link>

  <joint name="world_fixed" type="fixed">
    <origin xyz="0 0 0.7" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="pelvis"/>
  </joint>
-->

  <link name="base">
  </link>

  <joint name="base_fixed" type="fixed">
    <origin xyz="0 0 0.7" rpy="0 0 0"/>
    <parent link="base"/>
    <child link="pelvis"/>
  </joint>

  <!-- pelvis -->

  <link name="pelvis">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01" />
      </geometry>
    </collision>

    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://alice_description/meshes/prt_robot_pelvis_main.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="red"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>      
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
    </inertial>
  </link>
  

  <joint name="imu_fixed" type="fixed">
    <parent link="pelvis"/>
    <child link="imu_link"/>
    <origin xyz="0.0 0.0 0" rpy="0 0 0"/>
  </joint>

  <link name="imu_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>      
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
    </inertial>
  </link>

  <!-- chest -->
  <joint name="waist_pitch" type="revolute">
    <parent link="pelvis"/>
    <child link="waist_pitch_link"/>
    <origin xyz="0 0 0.20" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi/2}" upper="${pi}" />
    <dynamics damping = "0.7"/>
  </joint>
  
  <transmission name="waist_pitch_tran">
     <type>transmission_interface/SimpleTransmission</type>
     <joint name="waist_pitch">
       <hardwareInterface>EffortJointInterface</hardwareInterface>
     </joint>
     <actuator name="waist_pitch_motor">
       <hardwareInterface>EffortJointInterface</hardwareInterface>
       <mechanicalReduction>1</mechanicalReduction>
     </actuator>
  </transmission>

  <link name="waist_pitch_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01" />
      </geometry>
    </collision>

    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://alice_description/meshes/prt_robot_chest_2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="red"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>      
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
    </inertial>
  </link>
 
  <joint name="waist_yaw" type="revolute">
    <parent link="waist_pitch_link"/>
    <child link="waist_yaw_link"/>
    <origin xyz="0 0 0.144" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="6.5" effort="1000" lower="${-pi/2}" upper="${pi}" />
    <dynamics damping = "0.7"/>
   </joint>

  <transmission name="waist_yaw_tran">
     <type>transmission_interface/SimpleTransmission</type>
     <joint name="waist_yaw">
       <hardwareInterface>EffortJointInterface</hardwareInterface>
     </joint>
     <actuator name="waist_yaw_motor">
       <hardwareInterface>EffortJointInterface</hardwareInterface>
       <mechanicalReduction>1</mechanicalReduction>
     </actuator>
  </transmission>

  <link name="waist_yaw_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01" />
      </geometry>
    </collision>

    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://alice_description/meshes/prt_robot_chest_3.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="red"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>      
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
    </inertial>
  </link>

